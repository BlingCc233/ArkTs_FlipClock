{
	"app": {
		"bundleName": "com.blingcc.flipclock",
		"vendor": "publish",
		"versionCode": 1000000,
		"versionName": "1.0.0",
		"icon": "$media:app_icon",
		"label": "$string:app_name",
		"apiReleaseType": "Beta1",
		"compileSdkVersion": "5.0.0.25",
		"targetAPIVersion": 50000012,
		"minAPIVersion": 50000012,
